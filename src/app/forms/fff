<script>
$(document).bind("mobileinit", function () {
	$.mobile.ajaxEnabled = false;
});

function showLoader() {
	if (fdat.page_name != "landing_page" && fdat.page_name != "mobile") {
		if (fdat.partner != "ihl" || fdat.page_name != "vehicle_info"){
			$.mobile.loading( "show" );
		}
		var wait = setTimeout(function(){
			init();
	    	$.mobile.loading( "hide" );
		},1000);
	} else {
		init();
	}
}
$(document).ready(function() {
	$("button.show-loading-popup").click(function() {
		if (fdat.partner !="ihl" || fdat.page_name !="landing_page"){
			$.mobile.loading( "show" );
		}
	});
});
</script>
